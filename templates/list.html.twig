{% extends "base.html.twig" %}

{% block content %}
    <table>
        <thead>
        <tr>
            <th> url </th>
            <th> <a href="{{ path('bookmarks', paginator.getQueryParams(null, 'title', paginator.toggleOrder)) }}"> title </a></th>
        </tr>
        </thead>
        <tbody>
        {% for bookmark in paginator.getItems() %}
            <tr>
                <td>
                    {{ bookmark.url }}
                </td>
                <td>
                    {{ bookmark.title }}
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
