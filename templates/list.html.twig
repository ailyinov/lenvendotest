<!DOCTYPE html>
<html>
<body>
{% block body %}
    <table>
        <thead>
        <tr>
            <th> url </th>
            <th> title </th>
        </tr>
        </thead>
        <tbody>
        {% for bookmark in bookmarks %}
            <tr>
                <td>
                    {{ bookmark.url }}
                </td>
                <td>
                    {{ bookmark.title }}
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}

{% block javascripts %}{% endblock %}
</body>
<head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
    </head>
</html>
